{
  "name": "studiomitte/powermail-freebie",
  "type": "typo3-cms-extension",
  "description": "EShow content elements only to users who submitted a powermail form",
  "keywords": [
    "TYPO3",
    "extension",
    "powermail",
    "freebie"
  ],
  "homepage": "https://studiomitte.com",
  "authors": [
    {
      "name": "Georg Ringer",
      "role": "Developer"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^8.7 || ^9.5|| ^10.4",
    "in2code/powermail": "^6 || ^7 || ^8"
  },
  "autoload": {
    "psr-4": {
      "StudioMitte\\PowermailFreebie\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/powermail-freebie": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "powermail_freebie"
    }
  }
}
